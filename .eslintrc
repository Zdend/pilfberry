{
    "extends": "airbnb",
    "rules": {
        "react/jsx-tag-spacing": 2,
        "react/jsx-space-before-closing": 0,
        "indent": 0,
        "react/jsx-filename-extension": 0,
        "react/jsx-indent": 0,
        "react/jsx-indent-props": 0,
        "comma-dangle": 0,
        "padded-blocks": 0,
        "eol-last": 0,
        "import/no-extraneous-dependencies": 0,
        "arrow-body-style": 0,
        "import/prefer-default-export": 0,
        "func-names": 0,
        "prefer-arrow-callback": 0,
        "consistent-return": 0,
        "arrow-parens": 0,
        "prefer-template": 0
    }
}